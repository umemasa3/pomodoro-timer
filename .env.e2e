# E2Eテスト用環境変数
# デモモードを無効化してSupabaseを使用
VITE_DEMO_MODE=false

# テスト用Supabase設定（実際のテスト用プロジェクトを使用）
VITE_SUPABASE_URL=https://test-project.supabase.co
VITE_SUPABASE_ANON_KEY=test_anon_key

# アプリケーション設定
VITE_APP_ENV=test
VITE_APP_VERSION=0.0.0
VITE_APP_NAME=ポモドーロタイマー（E2Eテスト）

# 機能フラグ（テスト用に簡素化）
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_DEVTOOLS=true
VITE_FEATURE_OFFLINE_MODE=false
VITE_FEATURE_PUSH_NOTIFICATIONS=false
VITE_FEATURE_EXPORT_DATA=false

# キャッシュ設定
VITE_CACHE_VERSION=1